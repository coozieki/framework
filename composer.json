{
    "name": "coozieki/framework",
    "version": "0.1.1",
    "description": "Practice project to replicate PHP frameworks functionality",
    "license": "MIT",
    "authors": [
        {
            "name": "vadim.vorobyev",
            "email": "vadimvor.work@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~8.0",
        "coozieki/ioc-container": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "sempro/phpunit-pretty-print": "dev-develop",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Coozieki\\Framework\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/",
            "proj\\": "app/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
